<div class="welcome-text">
  <h1>friendlier<h1>
  <p>finding your new friend through your old friend</p>
</div>

<div class="welcome-image">
  <%= image_tag("welcome.png")%>
</div>

<div class="index-recent-activity">
  <p>Recent users</p>
  <% @user.each do |user| %>
    <div>
      <table>
        <tr>
          <td><%= link_to(image_tag("jgl.jpg"), user_path(user)) %></td>
          <td>
            <h3><%= link_to(user.name , user_path(user)) %></h3>
            <p><%= user.profile.age %></p>
            <p><%= user.profile.short_bio %></p>
            <p>Interests: <%= user.profile.interests %></p>
          </td>
        </tr>
      </table>
    </div>
  <% end %>
  <div class="border"></div>
</div>

<div></div>